<h2 id="login">Log in <br /> <br /></h2>


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <p>     <%= f.email_field :email, autofocus: true %></p>
  </div>

  <div class="field">
    <p> <%= f.password_field :password, autocomplete: "off" %></p>
  </div>

  <div class="actions">
    <p><%= f.submit "ENTER" %></p>
  </div>
<% end %>

<div id="login_deet">
  <h3>
   For access to the press portal, please email press@blackwater-la.com.
   <br /> <br />
   General inquiries should be directed to general@blackwater-la.com.
 </h3>
</div>

